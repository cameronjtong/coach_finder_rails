<% provide(:title, 'Coaches') %>
<h1>All Coaches</h1>
<div class=search_form>
    <%= form_tag(search_path, method: 'get') do %>
    <%= search_field_tag :search, params[:search], placeholder: 'Search coaches...',
        class: 'form-control' %>
        <div class='input-group-btn'>
          <%= button_tag 'Search', class: 'btn btn-info glyphicon glyphicon-search',
              name: nil %>
            </div>
          </div>
    <% end %>

<%= will_paginate %>

<ul class='users'>
  <%= render @users %>
</ul>

<%= will_paginate %>